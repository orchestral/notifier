{
    "name": "orchestra/notifier",
    "description": "User Notification Component for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "mailer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Notifier\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Notifier\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/encryption": "^6.0",
        "illuminate/mail": "^6.0",
        "illuminate/queue": "^6.0",
        "orchestra/contracts": "^4.0",
        "orchestra/memory": "^4.0",
        "orchestra/support": "^4.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2.1"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.0",
        "guzzlehttp/guzzle": "^6.0",
        "mockery/mockery": "^1.2.3",
        "orchestra/testbench": "^4.0.1"
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SES mail driver (^3.0).",
        "guzzlehttp/guzzle": "Required to use the Mailgun mail drivers (^6.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
